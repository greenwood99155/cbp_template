<main role="main" class="inner cover text-center">
  <h1 class="cover-heading">Rails News</h1>
  <div class="row">
    <% @tweets.each do |tweet| %>
      <div class="col-md-6">
        <div class="tech-news">
          <div class="card">
            <p><%= twitter_parser tweet %></p>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</main>
